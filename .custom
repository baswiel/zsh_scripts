#!/bin/zsh

#settings
alias zshrc="vim ~/.zshrc"
alias short="vim ~/.custom/.custom"
alias shortdir="cd ~/.custom"
alias gitfunctions= "vim ~/.custom/.git_functions"

alias deployscripts= "vim ~/.custom/.custom_deploy_scripts"

#Git aliases
alias gs="git status -sb"
alias g.="git add ."
alias gpl="git pull";
alias gd='git diff --output-indicator-new=" " --output-indicator-old=" "'
alias gl='git log --graph --all --pretty=format:"%C(magenta)%h %C(white) %an  %ar%C(blue)  %D%n%s%n"'

source ~/.custom/.git_functions

#Directories
alias code="cd ~/Code"

#k8s aliases
alias kgp="kubectl get pods"


alias zed="open -a /Applications/Zed.app -n"
alias pstorm="open -a /Applications/phpstorm.app -n"

#deployment scripts
source ~/.custom/.custom_deploy_scripts
